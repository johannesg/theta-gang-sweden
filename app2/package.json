{
  "name": "the-beth-stack",
  "module": "src/index.tsx",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"tsx watch src/index.tsx\" \"npm run tw:dev\"",
    "tw": "npx tailwindcss -i ./src/styles.css -o ./tailwind-gen/styles.css",
    "tw:dev": "npx tailwindcss -i ./src/styles.css -o ./tailwind-gen/styles.css --watch",
    "test": "jest",
    "testw": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/luxon": "^2.0.0",
    "@types/node": "^18.0.0",
    "concurrently": "^8.2.0",
    "jest": "^29.0.0",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.0.0",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@hono/node-server": "^1.1.1",
    "hono": "^3.6.3",
    "jstat": "^1.9.6",
    "luxon": "^2.0.0"
  }
}
