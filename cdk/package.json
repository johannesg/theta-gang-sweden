    "ts-node": "^10.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.19.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.19.0-alpha.0",
    "aws-cdk-lib": "2.19.0",
    "constructs": "^10.0.104",
    "source-map-support": "^0.5.21"
  }
